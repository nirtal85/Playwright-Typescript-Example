{
  "name": "playwright-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:dev": "playwright test --grep '@devRun'",
    "test:non-dev": "playwright test --grep-invert '@devRun'",
    "test:debug": "PWDEBUG=1 playwright test --grep '@devRun'",
    "install-browsers": "playwright install --with-deps",
    "report:serve": "allure serve allure-results",
    "codegen": "playwright codegen --viewport-size='800,600' https://google.com/",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Nir Tal",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "1.51.1",
    "@stylistic/eslint-plugin": "4.2.0",
    "@types/node": "22.13.14",
    "@types/ssh2-sftp-client": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "eslint": "9.23.0",
    "eslint-plugin-playwright": "2.2.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "ssh2-sftp-client": "^12.0.0",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@types/stripe": "8.0.416",
    "@visual-regression-tracker/agent-playwright": "5.3.1",
    "allure-playwright": "3.2.1",
    "dotenv": "16.4.7",
    "eslint-plugin-import": "2.31.0",
    "mailinator-client": "1.0.5",
    "mysql2": "^3.14.0",
    "stripe": "18.0.0",
    "xlsx": "0.18.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,yml}": [
